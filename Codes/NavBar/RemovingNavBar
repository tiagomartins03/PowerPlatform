Set(
    hidenavbar,
    Param("hidenavbar")
);
With(
    {AppLink: "AppPublishedLink"},
    If(
        hidenavbar = "false" Or IsBlank(hidenavbar),
        Launch(
            AppLink,
            {hidenavbar: true},
            LaunchTarget.Replace
        )
    )
)

 

